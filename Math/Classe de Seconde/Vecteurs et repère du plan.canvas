		{"id":"0de193a4c5d59cd3","type":"text","text":"$$\\vec{AB}=\\vec{CD}\\rightarrow\\text{Quadrilatère ABDC}$$","x":415,"y":-717,"width":309,"height":88,"color":"2"},
		{"id":"2e574eb9d71b883c","type":"text","text":"$$\\text{B milieu de [AB]}\\rightarrow\\vec{AB}=\\vec{BC}$$","x":415,"y":-597,"width":250,"height":90,"color":"2"},
